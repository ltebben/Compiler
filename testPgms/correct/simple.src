program test_program is
    variable i : integer;
    variable q : integer;
    variable out : bool;
    variable l : integer;
begin
    i := 1;
    q := 2;
    i := i*5+q;
    if (i < 8) then
        i := i-1;
    end if;
    out := putInteger(i);

    for (l:=4; l<6) 
        q:=q+l;
    end for;
    out := putInteger(q);
end program.
