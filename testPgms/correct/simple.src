program test_program is
    variable i : float;
    variable res : float;
    variable out : bool;
    variable str1 : string;
    variable str2 : string;
    variable test : bool;
begin
    str1 := "string asdfa";
    str2 := getString();
    out := putString(str2);
    test := str1 == str2;
    out := putBool(test);
    if (str1 != str2) then
        out := putString(str1);
    end if;
    out := putString(str2);
    
    i := 144.0;
    res := sqrt(i);
    out := putFloat(res);
end program.
