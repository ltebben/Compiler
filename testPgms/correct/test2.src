program test_program is
    variable fibb_result : integer;
    variable o : bool;
    procedure fibb : integer (variable n: integer)
        global variable x : integer;
        variable y : integer;
        variable result: integer;
    begin
        if (n < 0) then
            result := 0;
            return result;
        end if;
        if (n == 0) then
            result := 0;
            return result;
        end if;
        if (n == 1) then
            result := 1;
            return result;
        end if;
        
        x := fibb(n-1);
        y := fibb(n-2);
        result := x + y;
        return result;
    end procedure;
begin
    fibb_result := fibb(12);
    o := putInteger(fibb_result);
end program.
