program test_program is
    type fibb_result is integer;
    procedure fibb : integer (variable n: integer)
        variable x : integer;
        variable y : integer;
    begin
        if (n < 0) then
            result := -1;
            return result;
        end if;
        if (n == 0) then
            result := 0;
            return result;
        end if;
        if (n == 1) then
            result := 1;
            return result;
        end if;
        
        x := fibb(n-1);
        y := fibb(n-2);

        result := x + y;
        return result;
    end procedure;
begin
    fibb_result := fibb(12);
    final_ans := putInteger(fibb_result);
end program.
