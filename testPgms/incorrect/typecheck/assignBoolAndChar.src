program AssignFloatToInt is

global variable x : string;
variable res : bool;

variable arr : integer[3];

begin

x := "A";

res := x;  // Cannot assign char to bool
arr[1] := x; // Valid

if (1) then // Valid
    arr[2] := x;
end if;

x := res;  // Cannot assign bool to char
x := 24;  // Valid

putinteger(arr[2]);

end program.

